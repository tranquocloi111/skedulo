<livebill CustomerNumber="$CustomerNumber$">
    <sections>
        <section>
            <headertexts>
                <text>Billing Information</text>
            </headertexts>
            <rows>
                <row>
                    <item>Estimate of Usage Charges is current as of:</item>
                    <item align="right">$estimateOfUsageCharges$</item>
                </row>
                <row>
                    <item>Date of next invoice:</item>
                    <item align="right">$nextInvoiceDate$</item>
                </row>
                <row>
                    <item>Date next invoice is due for payment:</item>
                    <item align="right">$nextInvoiceDueDate$</item>
                </row>
            </rows>
        </section>
        <section>
            <headertexts>
                <text>Charges to Date</text>
                <text align="right">£40.00</text>
            </headertexts>
            <groups>
                <group>
                    <headertexts>
                        <text>Account Charges</text>
                        <text align="right">£0.00</text>
                    </headertexts>
                    <rows/>
                </group>
                <group>
                    <headertexts>
                        <text>Account Credits</text>
                        <text align="right">£0.00</text>
                    </headertexts>
                    <rows/>
                </group>
                <group>
                    <headertexts>
                        <text>Account Payments and Vouchers</text>
                        <text align="right">£-10.00</text>
                    </headertexts>
                    <rows>
                        <headerrow>
                            <item>From Date</item>
                            <item>Type</item>
                            <item>Reference</item>
                            <item align="right">Amount</item>
                        </headerrow>
                        <row>
                            <item>$dateFrom0$</item>
                            <item>Online Payment</item>
                            <item>$reference$</item>
                            <item align="right">£-10.00</item>
                        </row>
                    </rows>
                </group>
                <group invid="$invid$">
                    <headertexts>
                        <text>$subscriptionNumberValue$</text>
                        <text align="right">£50.00</text>
                    </headertexts>
                    <subgroups>
                        <subgroup>
                            <headertexts>
                                <text>Monthly Charges</text>
                                <text align="right">£50.00</text>
                            </headertexts>
                            <rows>
                                <headerrow>
                                    <item>Status</item>
                                    <item>Description</item>
                                    <item>Date From</item>
                                    <item>Date To</item>
                                    <item align="right">Amount</item>
                                </headerrow>
                                <row>
                                    <item>CURRENT</item>
                                    <item>£10 Tariff 12 Month Contract</item>
                                    <item>$dateFrom0$</item>
                                    <item>$dateTo0$</item>
                                    <item align="right">£10.00</item>
                                </row>
                                <row>
                                    <item>CURRENT</item>
                                    <item>£20 Tariff 12 Month Contract</item>
                                    <item>$dateFrom1$</item>
                                    <item>$dateTo1$</item>
                                    <item align="right">£20.00</item>
                                </row>
                                <row>
                                    <item>CURRENT</item>
                                    <item>£20 Tariff 12 Month Contract</item>
                                    <item>$dateFrom2$</item>
                                    <item>$dateTo2$</item>
                                    <item align="right">£20.00</item>
                                </row>
                            </rows>
                        </subgroup>
                        <subgroup>
                            <headertexts>
                                <text>Bundle Charges</text>
                                <text align="right">£0.00</text>
                            </headertexts>
                            <rows>
                                <headerrow>
                                    <item>Date From</item>
                                    <item>Date To</item>
                                    <item>Description</item>
                                    <item align="right">Amount</item>
                                </headerrow>
                                <row>
                                    <item>$dateFrom0$</item>
                                    <item>$dateTo0$</item>
                                    <item>£20 safety buffer for $subscriptionNumber$</item>
                                    <item align="right">£0.00</item>
                                </row>
                                <row>
                                    <item>$dateFrom1$</item>
                                    <item>$dateTo1$</item>
                                    <item>£10 safety buffer for $subscriptionNumber$</item>
                                    <item align="right">£0.00</item>
                                </row>
                                <row>
                                    <item>$dateFrom2$</item>
                                    <item>$dateTo2$</item>
                                    <item>£10 safety buffer for $subscriptionNumber$</item>
                                    <item align="right">£0.00</item>
                                </row>
                            </rows>
                        </subgroup>
                        <subgroup>
                            <headertexts>
                                <text>Usage Charges</text>
                                <text align="right">£0.00</text>
                            </headertexts>
                            <rows/>
                        </subgroup>
                        <subgroup>
                            <headertexts>
                                <text>Adjustments, Charges and Credits</text>
                                <text align="right">£0.00</text>
                            </headertexts>
                            <rows>
                                <headerrow>
                                    <item>Date From</item>
                                    <item>Date To</item>
                                    <item>Description</item>
                                    <item align="right">Amount</item>
                                </headerrow>
                                <row>
                                    <item>$dateFrom0$</item>
                                    <item/>
                                    <item>Nokia 2720</item>
                                    <item align="right">£0.00</item>
                                </row>
                            </rows>
                        </subgroup>
                        <subgroup>
                            <headertexts>
                                <text>Subscription Payments</text>
                                <text align="right">£0.00</text>
                            </headertexts>
                            <rows/>
                        </subgroup>
                    </subgroups>
                </group>
            </groups>
            <rows>
                <row>
                    <item class="bold">Current Bill Estimate:</item>
                    <item align="right" class="bold">£40.00</item>
                </row>
                <row>
                    <item>Overdue Balance:</item>
                    <item align="right">£0.00</item>
                </row>
                <row>
                    <item>Total Due:</item>
                    <item align="right">£40.00</item>
                </row>
            </rows>
        </section>
    </sections>
</livebill>